[nginx-botsearch]
enabled    = true
port       = http,https
filter     = nginx-botsearch
bantime    = 604800 ; 1 week
findtime   = 600	; 10 minutes
maxretry   = 1
logpath    = /var/log/nginx/access.log
	/var/log/nginx/**/access.log
